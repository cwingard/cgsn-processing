meta:
    # mooring deployment metadata
    name: &mooring CP10CNSM
    array: 'Coastal Pioneer MAB'
    deployment: {{ deployment | default('UNKNOWN_DEPLOYMENT') }}
    deploy_name: {{ deployment_name | default('UNKNOWN_DEPLOYMENT_NAME') }}
    category: 'Surface Mooring'
    subcategory: 'High Power'
    disposition: {{ disposition | default('UNKNOWN_DISPOSITION') }}
    start_date: {{ deployment_start | default('UNKNOWN_START_DATE') }}
    latitude: {{ latitude | default('UNKNOWN_LATITUDE') }}
    longitude: {{ longitude | default('UNKNOWN_LONGITUDE') }}
    deploy_cruise: {{ deployment_cruise | default('UNKNOWN_DEPLOY_CRUISE') }}
    stop_date: {{ deployment_end | default(None) }}
    watch_circle: 100
    water_depth: 32
    recover_cruise: {{ recovery_cruise | default(None) }}
paths:
    # absolute base paths to the raw, parsed, and processed data directories
    raw: /home/ooiuser/data/raw
    parsed: /home/ooiuser/data/parsed
    processed: /home/ooiuser/data/processed
platforms:
    # list of the subassemblies (aka platforms) and associated controllers, GPS
    # beacons, power systems, data loggers and instruments on that platform
    - platform1:
        # surface platform (BUOY)
        name: &buoy BUOY
        parent: *mooring
        depth: &depth 0
        pwrsys:
            name: &psc PSC
            refdes: None
            parent: *buoy
            controlled_by: None
            physical: *buoy
            depth: *depth
            serial: {{ psc_serial | default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ meta['name'].lower() }}/{{ meta['deploy_name'] }}/cg_data/syslog/*.syslog.log"
            parsed_data: "{{ meta['name'].lower() }}/{{ meta['deploy_name'] }}/buoy/pwrsys/*.syslog.json"
            dataset: {{ meta['name'] }}_BUOY_001_PWRSYS
        controller:
            name: &cpm1 CPM1
            refdes: {{ meta['name'] }}-SBC11-00-CPMENG000
            parent: *buoy
            controlled_by: *psc
            physical: *buoy
            depth: *depth
            uuid: {{ cpm1_uuid | default('UNKNOWN_UUID') | default('UNKNOWN_UUID') }}
            serial: {{ cpm1_serial | default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ meta['name'].lower() }}/{{ meta['deploy_name'] }}/cg_data/syslog/*.syslog.log"
            parsed_data: "{{ meta['name'].lower() }}/{{ meta['deploy_name'] }}/buoy/superv/{{ platforms['platform1']['controller']['name'].lower() }}/*.syslog.json"
            dataset: {{ meta['name'] }}_BUOY_001_SUPERV
        beacons:
            - beacon1:
                name: Rover1
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -4
                imei: {{ xeos1_imei | default('UNKNOWN_IMEI_NUMBER') }}
                serial: {{ xeos1_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ meta['name'].lower() }}/{{ meta['deploy_name'] }}/xeos_sbd/{{ xeos1_imei }}.log"
                parsed_data: "{{ meta['name'].lower() }}/{{ meta['deploy_name'] }}/buoy/xeos/rover1/{{ xeos1_imei }}.json"
                dataset: None
            - beacon2:
                name: Rover2
