            - sbd1:
                name: SBD_Primary
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                imei: {{ sbd1_imei|default('UNKNOWN_IMEI_NUMBER') }}
