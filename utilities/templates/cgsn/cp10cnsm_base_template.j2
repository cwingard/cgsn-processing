# baseline mooring configuration template for CP10CNSM
meta:
    # mooring deployment metadata
    # simple macro to convert the deployment name to an integer
    name: {{ mooring }}
    array: 'Coastal Pioneer MAB'
    deployment: {{ deployment }}
    deploy_name: {{ deployment_name }}
    category: 'Surface Mooring'
    subcategory: 'High Power'
    disposition: {{ disposition|default('UNKNOWN_DISPOSITION') }}
    start_date: {{ deployment_start|default('UNKNOWN_START_DATE') }}
    latitude: {{ latitude|default('UNKNOWN_LATITUDE') }}
    longitude: {{ longitude|default('UNKNOWN_LONGITUDE') }}
    deploy_cruise: {{ deployment_cruise|default('UNKNOWN_DEPLOY_CRUISE') }}
    recover_cruise: {{ recovery_cruise|default(None) }}
    stop_date: {{ deployment_end|default(None) }}
    watch_circle: 100
    water_depth: {{ site_depth }}
paths:
    # absolute base paths to the raw, parsed, and processed data directories
    raw: /home/ooiuser/data/raw
    parsed: /home/ooiuser/data/parsed
    processed: /home/ooiuser/data/processed
platforms:
    # list of the subassemblies (aka platforms) and associated controllers, GPS
    # beacons, power systems, data loggers and instruments on the platform
    - platform1:
        # surface platform (BUOY)
        name: &buoy BUOY
        parent: {{ mooring }}
        depth: &depth 0
        pwrsys:
            # Primary mooring power system controller (PSC)
            name: &psc PSC
            refdes: None
            parent: *buoy
            controlled_by: None
            physical: *buoy
            depth: *depth
            uuid: {{ psc_uuid|default('UNKNOWN_UUID') }}
            serial: {{ psc_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/syslog/*.syslog.log"
            parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/pwrsys/*.syslog.json"
            dataset: {{ mooring }}_BUOY_001_PWRSYS
        controller:
            # Primary mooring power and communications controller (CPM1)
            {% set cpm = 'CPM1' -%}
            name: &cpm1 {{ cpm }}
            refdes: {{ mooring }}-SBC11-00-CPMENG000
            parent: *buoy
            controlled_by: *psc
            physical: *buoy
            depth: *depth
            uuid: {{ buoy_cpm_uuid|default('UNKNOWN_UUID') }}
            serial: {{ buoy_cpm_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/syslog/*.syslog.log"
            parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/superv/{{ cpm|lower }}/*.syslog.json"
            dataset: {{ mooring }}_BUOY_001_SUPERV
        beacons:
            # GPS beacons used to independently track mooring location
            - beacon1:  #  surface beacon, watch circle enabled
                {% set imei = xeos1_imei|default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Rover1' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -4
                imei: {{ imei }}
                serial: {{ xeos1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/xeos/{{ name|lower }}/{{ imei }}.json"
                dataset: None
            - beacon2:  #  surface beacon, watch circle enabled
                {% set imei = xeos2_imei|default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Rover2' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -4
                imei: {{ xeos }}
                serial: {{ xeos2_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/xeos/{{ name|lower }}/{{ imei }}.json"
                dataset: None
            - beacon3:  #  subsurface beacon, alarm if seen after deployment
                {% set imei = xeos3_imei|default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Apollo' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: 1.5
                imei: {{ xeos }}
                serial: {{ xeos3_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/xeos/{{ name|lower }}/{{ imei }}.json"
                dataset: None
        loggers:
            # Data loggers controlled by the buoy platform controller
            - logger1:
                {% set dcl1 = 'DCL11' -%}
                name: {{ dcl1 }}
                refdes: {{ mooring }}-SBD11-00-DCLENG000
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: *depth
                uuid: {{ dcl1_uuid|default('UNKNOWN_UUID') }}
                serial: {{ dcl1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/superv/{{ dcl1|lower }}/*.syslog.json"
                dataset: {{ mooring }}_BUOY_002_SUPERV
            - logger2:
                {% set dcl2 = 'DCL12' -%}
                name: {{ dcl2 }}
                refdes: {{ mooring }}-SBD12-00-DCLENG000
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: *depth
                uuid: {{ dcl2_uuid|default('UNKNOWN_UUID') }}
                serial: {{ dcl2_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl2|lower }}/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/superv/{{ dcl2|lower }}/*.syslog.json"
                dataset: {{ mooring }}_BUOY_003_SUPERV
        instrmts:
            # Instruments controlled and logged by the buoy CPM, independent of the other data loggers
            - gps:
                name: GPS
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                uuid: {{ gps_uuid|default('UNKNOWN_UUID') }}
                serial: {{ gps_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/gps/*.syslog.json"
                dataset: {{ mooring }}_BUOY_001_GPS
            - iridium:
                name: IRIDIUM
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                uuid: {{ irid_uuid|default('UNKNOWN_UUID') }}
                serial: {{ irid_serial|default('UNKNOWN_SERIAL_NUMBER') }}
