                parent: *metbk
                controlled_by: *metbk
                physical: *buoy
                depth: -4
                port: 6
                ui: False
                uuid: {{ metswr_uuid|default('UNKNOWN_UUID') }}
                serial: {{ metswr_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            - metwnd:
                name: METWND
                refdes: {{ mooring }}-SBD11-06-METBKA007
                parent: *metbk
                controlled_by: *metbk
                physical: *buoy
                depth: -4
                port: 6
                ui: False
                uuid: {{ metwnd_uuid|default('UNKNOWN_UUID') }}
                serial: {{ metwnd_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            - methtr:
                name: METHTR
                refdes: None
                parent: *dcl1
                controlled_by: *dcl1
                physical: *buoy
                depth: *depth
                port: 7
                uuid: {{ methtr_uuid|default('UNKNOWN_UUID') }}
