                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/adcpt/*.adcpt1.json"
                dataset: {{ mooring }}_NSIF_001_ADCPT
            - velpt:
                name: VELPT
                refdes: {{ mooring }}-RID26-04-VELPTA000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 4
                uuid: {{ nsif_velpt_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_velpt_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/velpt1/*.velpt1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/velpt/*.velpt1.json"
                dataset: {{ mooring }}_NSIF_001_VELPT
            - acomm:
                name: UCSPP
                refdes: None
                parent: *dcl1
