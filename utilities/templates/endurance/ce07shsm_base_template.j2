                dataset: {{ mooring }}_NSIF_002_NUTNR
            - spkir:
                name: SPKIR
                refdes: {{ mooring }}-RID26-08-SPKIRB000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 8
                uuid: {{ nsif_spkir_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_spkir_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/spkir/*.spkir.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/spkir/*.spkir.json"
                dataset: {{ mooring }}_NSIF_001_SPKIR
            # Instruments controlled and logged by the second NSIF data logger
            - optaa:
                name: OPTAA
                refdes: {{ mooring }}-RID27-01-OPTAAD000
                parent: *dcl2
                controlled_by: *dcl2
                physical: *nsif
                depth: *depth
                port: 1
                uuid: {{ nsif_optaa_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_optaa_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl2|lower }}/optaa1_decimated/*.optaa1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/optaa/*.optaa1.json"
