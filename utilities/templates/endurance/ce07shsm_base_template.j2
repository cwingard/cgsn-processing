    - platform2:
        # midwater platform (NSIF)
        name: &nsif NSIF
        parent: {{ mooring }}
        depth: &depth 7
        pwrsys: None
        controller:
            # Platform controller for the NSIF
            {% set cpm = 'CPM2' -%}
            name: &cpm2 {{ cpm }}
            refdes: {{ mooring }}-RIC21-00-CPMENG000
            parent: *nsif
            controlled_by: *cpm1
            physical: *nsif
            depth: *depth
            uuid: {{ nsif_cpm_uuid|default('UNKNOWN_UUID') }}
            serial: {{ nsif_cpm_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ cpm|lower }}/syslog/*.syslog.log"
