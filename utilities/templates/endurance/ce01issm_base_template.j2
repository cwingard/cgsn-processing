# baseline mooring configuration template for CE01ISSM
meta:
    name: {{ mooring }}
    array: 'Coastal Endurance'
    deployment: {{ deployment }}
    deploy_name: {{ deployment_name }}
    category: 'Surface Mooring'
    subcategory: 'High Power'
    disposition: {{ disposition|default('UNKNOWN_DISPOSITION') }}
    start_date: {{ deployment_start|default('UNKNOWN_START_DATE') }}
    latitude: {{ latitude|default('UNKNOWN_LATITUDE') }}
    longitude: {{ longitude|default('UNKNOWN_LONGITUDE') }}
    deploy_cruise: {{ deployment_cruise|default('UNKNOWN_DEPLOY_CRUISE') }}
    recover_cruise: {{ recovery_cruise|default(None) }}
    stop_date: {{ deployment_end|default(None) }}
    watch_circle: 250
    water_depth: {{ site_depth }}
paths:
    # absolute base paths to the raw, parsed, and processed data directories
    raw: /home/ooiuser/data/raw
    parsed: /home/ooiuser/data/parsed
    processed: /home/ooiuser/data/processed
platforms:
    # list of the subassemblies (aka platforms) and associated controllers, GPS
    # beacons, power systems, data loggers and instruments on the platform
    - platform1:
        # surface platform (BUOY)
        name: &buoy BUOY
        parent: {{ mooring }}
        depth: &depth 0
        pwrsys: None
        controller:
            # Primary mooring power and communications controller (CPM1)
            {% set cpm = 'CPM1' -%}
            name: &cpm1 {{ cpm }}
            refdes: {{ mooring }}-SBC11-00-CPMENG000
            parent: *buoy
            controlled_by: *psc
            physical: *buoy
            depth: *depth
            uuid: {{ buoy_cpm_uuid|default('UNKNOWN_UUID') }}
            serial: {{ buoy_cpm_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/syslog/*.syslog.log"
            parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/superv/{{ cpm|lower }}/*.syslog.json"
            dataset: {{ mooring }}_BUOY_001_SUPERV
        beacons:
            # GPS beacons used to independently track mooring location
            - beacon1:  #  surface beacon, watch circle enabled
                {% set imei = xeos1_imei|default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Kilo1' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -1
                imei: {{ imei }}
                serial: {{ xeos1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/xeos/{{ name|lower }}/{{ imei }}.json"
                dataset: None
            - beacon2:  #  surface beacon, watch circle enabled
                {% set imei = xeos2_imei|default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Kilo2' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -1
                imei: {{ xeos }}
                serial: {{ xeos2_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/xeos/{{ name|lower }}/{{ imei }}.json"
                dataset: None
        loggers:
            # Data loggers controlled by the buoy platform controller
            - logger1:
                {% set dcl1 = 'DCL17' -%}
                name: {{ dcl1 }}
                refdes: {{ mooring }}-SBD17-00-DCLENG000
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: *depth
                uuid: {{ dcl1_uuid|default('UNKNOWN_UUID') }}
                serial: {{ dcl1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/superv/{{ dcl1|lower }}/*.syslog.json"
                dataset: {{ mooring }}_BUOY_002_SUPERV
        instrmts:
            # Instruments controlled and logged by the buoy CPM, independent of the other data loggers
            - gps:
                name: GPS
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -1
                uuid: {{ gps_uuid|default('UNKNOWN_UUID') }}
                serial: {{ gps_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/gps/*.syslog.json"
                dataset: {{ mooring }}_BUOY_001_GPS
            - iridium:
                name: IRIDIUM
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -1
                uuid: {{ irid_uuid|default('UNKNOWN_UUID') }}
                serial: {{ irid_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/iridium/*.syslog.json"
                dataset: {{ mooring }}_BUOY_001_IRIDIUM
            - sbd:
                name: SBD_Primary
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -1
                imei: {{ sbd1_imei|default('UNKNOWN_IMEI_NUMBER') }}
                serial: {{ sbd1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/irid_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/sbd_primary/*.json"
                dataset: None
            # Instruments controlled and logged by the buoy data logger
            - mopak:
                name: MOPAK
                refdes: {{ mooring }}-SBD11-01-MOPAK0000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *buoy
                port: 1
                depth: 0
                uuid: {{ mopak_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mopak_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/mopak/*.mopak.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/mopak/*.mopak.json"
                dataset: {{ mooring }}_BUOY_001_MOPAK
            - velpt:
                name: VELPT
                refdes: {{ mooring }}-SBD17-04-VELPTA000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *buoy
                depth: *depth
                port: 1.5
                uuid: {{ buoy_velpt_uuid|default('UNKNOWN_UUID') }}
                serial: {{ buoy_velpt_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/velpt1/*.velpt1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/velpt/*.velpt1.json"
                dataset: {{ mooring }}_BUOY_001_VELPT
            - acomm:
                name: UCSPP
                refdes: None
                parent: *dcl1
                controlled_by: *dcl1
                physical: *buoy
                depth: 1.5
                port: 5
                uuid: {{ acomm_uuid|default('UNKNOWN_UUID') }}
                serial: {{ acomm_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/ucspp/*.ucspp.log"
                parsed_data: None
                dataset: None
            - ctdbp:
                name: &ctd CTDBP
                refdes: {{ mooring }}-SBD17-06-CTDBPC000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *buoy
                depth: 1.5
                port: 6
                uuid: {{ buoy_ctdbp_uuid|default('UNKNOWN_UUID') }}
                serial: {{ buoy_ctdbp_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/ctdbp3/*.ctdbp3.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/buoy/ctdbp/*.ctdbp3.json"
                dataset: {{ mooring }}_BUOY_001_CTDBP_FLORT
            - flort:
                name: FLORT
                refdes: {{ mooring }}-RID16-03-FLORTD000
                parent: *ctd
                controlled_by: *ctd
                physical: *buoy
                depth: 1.5
                port: 6
                ui: False
                uuid: {{ buoy_flort_uuid|default('UNKNOWN_UUID') }}
                serial: {{ buoy_flort_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/ctdbp3/*.ctdbp3.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/ctdbp/*.ctdbp3.json"
                dataset: {{ mooring }}_BUOY_001_CTDBP_FLORT
    - platform2:
        # midwater platform (NSIF)
        name: &nsif NSIF
        parent: {{ mooring }}
        depth: &depth 7
        pwrsys: None
        controller:
            # Platform controller for the NSIF
            {% set cpm = 'CPM2' -%}
            name: &cpm2 {{ cpm }}
            refdes: {{ mooring }}-RIC21-00-CPMENG000
            parent: *nsif
            controlled_by: *cpm1
            physical: *nsif
            depth: *depth
            uuid: {{ nsif_cpm_uuid|default('UNKNOWN_UUID') }}
            serial: {{ nsif_cpm_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ cpm|lower }}/syslog/*.syslog.log"
            parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/superv/{{ cpm|lower }}/*.syslog.json"
            dataset: {{ mooring }}_NSIF_001_SUPERV
        beacons: None
        loggers:
            # Data logger on the NSIF controlled by the buoy platform controller
            - logger1:
                {% set dcl1 = 'DCL16' -%}
                name: &dcl1 {{ dcl1 }}
                refdes: {{ mooring }}-RID16-00-DCLENG000
                parent: *cpm1
                controlled_by: *cpm1
                physical: *nsif
                depth: *depth
                uuid: {{ nsif_dcl1_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_dcl1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/superv/{{ dcl1|lower }}/*.syslog.json"
                dataset: {{ mooring }}_NSIF_001_SUPERV
        instrmts:
            # Instruments controlled and logged by the NSIF data logger
            - optaa:
                name: OPTAA
                refdes: {{ mooring }}-RID16-01-OPTAAD000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 1
                uuid: {{ nsif_optaa_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_optaa_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/optaa1_decimated/*.optaa1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/optaa/*.optaa1.json"
                dataset: {{ mooring }}_NSIF_001_OPTAA
            - flort:
                name: FLORT
                refdes: {{ mooring }}-RID16-02-FLORTD000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 2
                uuid: {{ flort_uuid|default('UNKNOWN_UUID') }}
                serial: {{ flort_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/flort/*.flort.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/flort/*.flort.json"
                dataset: {{ mooring }}_NSIF_001_FLORT
            - ctdbp:
                name: &ctd CTDBP
                refdes: {{ mooring }}-RID16-03-CTDBPC000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 3
                uuid: {{ nsif_ctdbp_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_ctdbp_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/ctdbp1/*.ctdbp1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/ctdbp/*.ctdbp1.json"
                dataset: {{ mooring }}_NSIF_001_CTDBP_DOSTA
            - dosta:
                name: DOSTA
                refdes: {{ mooring }}-RID16-03-DOSTAD000
                parent: *ctd
                controlled_by: *ctd
                physical: *nsif
                depth: *depth
                port: 3
                ui: False
                uuid: {{ nsif_dosta_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_dosta_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/ctdbp1/*.ctdbp1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/ctdbp/*.ctdbp1.json"
                dataset: {{ mooring }}_NSIF_001_CTDBP_DOSTA
            - velpt:
                name: VELPT
                refdes: {{ mooring }}-RID16-04-VELPTA000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 4
                uuid: {{ nsif_velpt_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_velpt_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/velpt1/*.velpt1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/velpt/*.velpt1.json"
                dataset: {{ mooring }}_NSIF_001_VELPT
            - phsen:
                name: PHSEN
                refdes: {{ mooring }}-RID16-06-PHSENG000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 6
                uuid: {{ nsif_phsen_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_phsen_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/phsen1/*.phsen1.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/phsen/*.phsen1.json"
                dataset: {{ mooring }}_NSIF_001_PHSEN
            - nutnr:
                name: NUTNR
                refdes: {{ mooring }}-RID16-07-NUTNRB000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 7
                uuid: {{ nutnr_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nutnr_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/nutnr/*.nutnr.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/nutnr/*.nutnr.json"
                dataset: {{ mooring }}_NSIF_002_NUTNR
            - spkir:
                name: SPKIR
                refdes: {{ mooring }}-RID16-08-SPKIRB000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *nsif
                depth: *depth
                port: 8
                uuid: {{ nsif_spkir_uuid|default('UNKNOWN_UUID') }}
                serial: {{ nsif_spkir_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/spkir/*.spkir.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/nsif/spkir/*.spkir.json"
                dataset: {{ mooring }}_NSIF_001_SPKIR
    - platform3:
        # seafloor platform (MFN)
        name: &mfn MFN
        parent: {{ mooring }}
        depth: &depth {{ site_depth }}
        pwrsys:
            # High-voltage MFN power converter
            {% set cpm = 'CPM3' -%}
            name: &mpea MPEA
            refdes: None
            parent: *mfn
            controlled_by: *psc
            physical: *mfn
            depth: *depth
            uuid: {{ mpea_uuid|default('UNKNOWN_UUID') }}
            serial: {{ mpea_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ cpm|lower }}/syslog/*.syslog.log"
            parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/mpea/*.syslog.json"
            dataset: {{ mooring }}_MFN_001_MPEA
        controller:
            # Platform controller for the MFN
            name: &cpm3 {{ cpm }}
            refdes: {{ mooring }}-MFC31-00-CPMENG000
            parent: *mfn
            controlled_by: *mpea
            physical: *mfn
            depth: *depth
            uuid: {{ mfn_cpm_uuid|default('UNKNOWN_UUID') }}
            serial: {{ mfn_cpm_serial|default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ cpm|lower }}/syslog/*.syslog.log"
            parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/superv/{{ cpm|lower }}/*.syslog.json"
            dataset: {{ mooring }}_MFN_001_SUPERV
        beacons: None
        loggers:
            # Data loggers controlled by the MFN platform controller
            - logger1:
                {% set dcl1 = 'DCL36' -%}
                name: {{ dcl1 }}
                refdes: {{ mooring }}-MFD35-00-DCLENG000
                parent: *cpm3
                controlled_by: *cpm3
                physical: *mfn
                depth: *depth
                uuid: {{ mfn_dcl1_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mfn_dcl1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/superv/{{ dcl1|lower }}/*.syslog.json"
                dataset: {{ mooring }}_MFN_002_SUPERV
            - logger2:
                {% set dcl1 = 'DCL37' -%}
                name: {{ dcl1 }}
                refdes: {{ mooring }}-MFD37-00-DCLENG000
                parent: *cpm3
                controlled_by: *cpm3
                physical: *mfn
                depth: *depth
                uuid: {{ mfn_dcl1_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mfn_dcl1_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/syslog/*.syslog.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/superv/{{ dcl1|lower }}/*.syslog.json"
                dataset: {{ mooring }}_MFN_003_SUPERV
        instrmts:
            # Instruments controlled and logged by the first MFN data logger
            - vel3d:
                name: VEL3D
                refdes: {{ mooring }}-MFD35-01-VEL3DD000
                parent: None
                controlled_by: None
                physical: *mfn
                depth: *depth
                port: None
                uuid: {{ mfn_prtsz_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mfn_prtsz_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            - presf:
                name: PRESF
                refdes: {{ mooring }}-MFD35-02-PRESFE000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *mfn
                depth: *depth
                port: 2
                uuid: {{ presf_uuid|default('UNKNOWN_UUID') }}
                serial: {{ presf_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/presf/*.presf.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/presf/*.presf.json"
                dataset: {{ mooring }}_MFN_001_PRESF
            - adcpt:
                name: ADCPT
                refdes: {{ mooring }}-MFD37-02-ADCPTC000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *mfn
                depth: *depth
                port: 4
                uuid: {{ adcpt_uuid|default('UNKNOWN_UUID') }}
                serial: {{ adcpt_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/adcpt2/*.adcpt2.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/adcp/*.adcpt2.json"
                dataset: {{ mooring }}_MFN_001_ADCPT
            - pco2w:
                name: PCO2W
                refdes: {{ mooring }}-MFD35-05-PCO2WB000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *mfn
                depth: *depth
                port: 5
                uuid: {{ pco2w_uuid|default('UNKNOWN_UUID') }}
                serial: {{ pco2w_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/pco2w/*.pco2w.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/pco2w/*.pco2w.json"
                dataset: {{ mooring }}_MFN_001_PCO2W
            - phsen:
                name: PHSEN
                refdes: {{ mooring }}-MFD35-06-PHSEND000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *mfn
                depth: *depth
                port: 6
                uuid: {{ mfn_phsen_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mfn_phsen_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/phsen2/*.phsen2.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/phsen/*.phsen2.json"
                dataset: {{ mooring }}_MFN_001_PHSEN
            # Instruments controlled and logged by the second MFN data logger
            - optaa:
                name: OPTAA
                refdes: {{ mooring }}-MFD37-01-OPTAAD000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *mfn
                depth: *depth
                port: 1
                uuid: {{ mfn_optaa_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mfn_optaa_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/optaa2_decimated/*.optaa2.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/optaa/*.optaa2.json"
                dataset: {{ mooring }}_MFN_001_OPTAA
            - ctdbp:
                name: &ctd CTDBP
                refdes: {{ mooring }}-MFD37-03-CTDBPC000
                parent: *dcl1
                controlled_by: *dcl1
                physical: *mfn
                depth: *depth
                port: 3
                uuid: {{ mfn_ctdbp_uuid|default('UNKNOWN_UUID') }}
                serial: {{ mfn_ctdbp_serial|default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring|lower }}/{{ deployment_name }}/cg_data/{{ dcl1|lower }}/ctdbp2/*.ctdbp2.log"
                parsed_data: "{{ mooring|lower }}/{{ deployment_name }}/mfn/ctdbp/*.ctdbp2.json"
                dataset: {{ mooring }}_MFN_001_CTDBP_DOSTA
            - dosta:
                name: DOSTA
                refdes: {{ mooring }}-MFD37-03-DOSTAD000
                parent: *ctd
                controlled_by: *ctd
                physical: *mfn
                depth: *depth
