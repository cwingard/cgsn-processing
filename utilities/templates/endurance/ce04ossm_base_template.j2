# baseline mooring configuration template for CE04OSSM
meta:
    name: {{ mooring }}
    array: 'Coastal Endurance'
    deployment: {{ deployment }}
    deploy_name: {{ deployment_name }}
    category: 'Surface Mooring'
    subcategory: 'Standard Power'
    disposition: {{ disposition | default('UNKNOWN_DISPOSITION') }}
    start_date: !!str {{ deployment_start | default('UNKNOWN_START_DATE') }}
    latitude: {{ latitude | default('UNKNOWN_LATITUDE') }}
    longitude: {{ longitude | default('UNKNOWN_LONGITUDE') }}
    deploy_cruise: {{ deployment_cruise | default('UNKNOWN_DEPLOY_CRUISE') }}
    recover_cruise: {{ recovery_cruise | default(None) }}
    stop_date: !!str {{ deployment_end | default(None) }}
    watch_circle: 250
    water_depth: {{ site_depth }}
paths:
    # absolute base paths to the raw, parsed, and processed data directories
    raw: /home/ooiuser/data/raw
    parsed: /home/ooiuser/data/parsed
    processed: /home/ooiuser/data/processed
platforms:
    # list of the subassemblies (aka platforms) and associated controllers, GPS
    # beacons, power systems, data loggers and instruments on the platform
    - platform1:
        # surface platform (BUOY)
        name: &buoy BUOY
        parent: {{ mooring }}
        depth: &depth1 0
        pwrsys:
            # Primary mooring power system controller (PSC)
            name: &psc PSC
            refdes: None
            parent: *buoy
            controlled_by: None
            physical: *buoy
            depth: *depth1
            uuid: {{ psc_uuid | default('UNKNOWN_UUID') }}
            serial: {{ psc_serial | default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/superv/*.superv.log"
            parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/pwrsys/*.superv.json"
            dataset: {{ mooring }}-BUOY-001-PWRSYS
        controller:
            # Primary mooring power and communications controller (CPM1)
            {% set cpm = 'CPM1' -%}
            name: &cpm1 {{ cpm }}
            refdes: {{ mooring }}-SBC11-00-CPMENG000
            parent: *buoy
            controlled_by: *psc
            physical: *buoy
            depth: *depth1
            uuid: {{ cpm1_uuid | default('UNKNOWN_UUID') }}
            serial: {{ cpm1_serial | default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/superv/*.superv.log"
            parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/superv/{{ cpm | lower }}/*.superv.json"
            dataset: {{ mooring }}-BUOY-001-SUPERV
        beacons:
            # GPS beacons used to independently track mooring location
            - beacon1:  #  surface beacon, watch circle enabled
                {% set imei = xeos1_imei | default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Rover1' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -4
                imei: {{ imei }}
                serial: {{ xeos1_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/xeos/{{ name | lower }}/{{ imei }}.json"
                dataset: None
            - beacon2:  #  surface beacon, watch circle enabled
                {% set imei = xeos2_imei | default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Rover2' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: -4
                imei: {{ imei }}
                serial: {{ xeos2_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/xeos/{{ name | lower }}/{{ imei }}.json"
                dataset: None
            - beacon3:  #  subsurface beacon, alarm if seen after deployment
                {% set imei = xeos3_imei | default('UNKNOWN_IMEI_NUMBER') -%}
                {% set name = 'Kilo' -%}
                name: {{ name }}
                refdes: None
                parent: *buoy
                controlled_by: None
                physical: *buoy
                depth: 1.5
                imei: {{ imei }}
                serial: {{ xeos3_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/xeos_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/xeos/{{ name | lower }}/{{ imei }}.json"
                dataset: None
        loggers:
            # Data loggers controlled by the buoy platform controller
            - logger1:
                {% set dcl11 = 'DCL11' -%}
                name: &dcl11 {{ dcl11 }}
                refdes: {{ mooring }}-SBD11-00-DCLENG000
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: *depth1
                uuid: {{ dcl11_uuid | default('UNKNOWN_UUID') }}
                serial: {{ dcl11_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl11 | lower }}/superv/*.superv.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/superv/{{ dcl11 | lower }}/*.superv.json"
                dataset: {{ mooring }}-BUOY-002-SUPERV
            - logger2:
                {% set dcl12 = 'DCL12' -%}
                name: &dcl12 {{ dcl12 }}
                refdes: {{ mooring }}-SBD12-00-DCLENG000
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: *depth1
                uuid: {{ dcl12_uuid | default('UNKNOWN_UUID') }}
                serial: {{ dcl12_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl12 | lower }}/superv/*.superv.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/superv/{{ dcl12 | lower }}/*.superv.json"
                dataset: {{ mooring }}-BUOY-003-SUPERV
        instrmts:
            # Instruments controlled and logged by the buoy CPM, independent of the other data loggers
            - gps:
                name: GPS
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                uuid: {{ gps_uuid | default('UNKNOWN_UUID') }}
                serial: {{ gps_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/superv/*.superv.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/gps/*.superv.json"
                dataset: {{ mooring }}-BUOY-001-GPS
            - iridium:
                name: IRIDIUM
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                uuid: {{ irid_uuid | default('UNKNOWN_UUID') }}
                serial: {{ irid_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/superv/*.superv.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/iridium/*.superv.json"
                dataset: {{ mooring }}-BUOY-001-IRIDIUM
            - sbd1:
                {% set imei = sbd1_imei | default('UNKNOWN_IMEI_NUMBER') -%}
                name: SBD_Primary
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                imei: {{ imei }}
                serial: {{ sbd1_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/irid_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/sbd_primary/*.json"
                dataset: None
            - sbd2:
                {% set imei = sbd2_imei | default('UNKNOWN_IMEI_NUMBER') -%}
                name: SBD_Backup
                refdes: None
                parent: *cpm1
                controlled_by: *cpm1
                physical: *buoy
                depth: -4
                imei: {{ imei }}
                serial: {{ sbd2_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/irid_sbd/{{ imei }}.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/sbd_backup/*.json"
                dataset: None
            # Instruments controlled and logged by the first buoy data logger
            - mopak:
                name: MOPAK
                refdes: {{ mooring }}-SBD11-01-MOPAK0000
                parent: *dcl11
                controlled_by: *dcl11
                physical: *buoy
                port: 1
                depth: *depth1
                uuid: {{ mopak_uuid | default('UNKNOWN_UUID') }}
                serial: {{ mopak_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl11 | lower }}/mopak/*.mopak.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/mopak/*.mopak.json"
                dataset: {{ mooring }}-BUOY-001-MOPAK
            - hyd1:
                name: HYDGN1
                refdes: {{ mooring }}-SBD11-02-HYDGN0000
                parent: *dcl11
                controlled_by: *dcl11
                physical: *buoy
                depth: *depth1
