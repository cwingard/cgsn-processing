                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl26 | lower }}/nutnr/*.nutnr.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/nutnr/*.nutnr.json"
                dataset: {{ mooring }}-NSIF-002-NUTNR
            - spkir:
                name: SPKIR
                refdes: {{ mooring }}-RID26-08-SPKIRB000
                parent: *dcl26
                controlled_by: *dcl26
