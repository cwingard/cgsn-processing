                controlled_by: *cpm2
                physical: *nsif
                depth: *depth2
                uuid: {{ dcl27_uuid | default('UNKNOWN_UUID') }}
                serial: {{ dcl27_serial | default('UNKNOWN_SERIAL_NUMBER') }}
