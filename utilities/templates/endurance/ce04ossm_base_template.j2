                parent: *metbk
                controlled_by: *metbk
                physical: *buoy
                depth: -4
                ui: False
                uuid: {{ metprc_uuid | default('UNKNOWN_UUID') }}
                serial: {{ metprc_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            - metswr:
                name: METSWR
                refdes: {{ mooring }}-SBD11-06-METBKA006
                parent: *metbk
                controlled_by: *metbk
                physical: *buoy
                depth: -4
                ui: False
                uuid: {{ metswr_uuid | default('UNKNOWN_UUID') }}
                serial: {{ metswr_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            - metwnd:
                name: METWND
                refdes: {{ mooring }}-SBD11-06-METBKA007
                parent: *metbk
                controlled_by: *metbk
                physical: *buoy
                depth: -4
                ui: False
                uuid: {{ metwnd_uuid | default('UNKNOWN_UUID') }}
                serial: {{ metwnd_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            - methtr:
                name: METHTR
                refdes: None
                parent: *dcl11
                controlled_by: *dcl11
                physical: *buoy
                depth: *depth1
                port: 7
                uuid: {{ methtr_uuid | default('UNKNOWN_UUID') }}
                serial: {{ methtr_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
                dataset: None
            # Instruments controlled and logged by the second buoy data logger
            - hyd2:
                name: HYDGN2
                refdes: {{ mooring }}-SBD12-03-HYDGN0000
                parent: *dcl12
                controlled_by: *dcl12
                physical: *buoy
                depth: *depth1
                port: 3
                uuid: {{ hyd2_uuid | default('UNKNOWN_UUID') }}
                serial: {{ hyd2_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl12 | lower }}/hyd2/*.hyd2.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/hydgn2/*.hyd2.json"
                dataset: {{ mooring }}-BUOY-002-HYDGN
            - pco2a:
                name: PCO2A
                refdes: {{ mooring }}-SBD12-04-PCO2AA000
                parent: *dcl12
                controlled_by: *dcl12
                physical: *buoy
                depth: *depth1
