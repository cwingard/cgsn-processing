                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/wavss/*.wavss.json"
                dataset: {{ mooring }}-BUOY-001-WAVSS
    - platform2:
        # midwater platform (NSIF)
        name: &nsif NSIF
        parent: {{ mooring }}
        depth: &depth2 7
        pwrsys: None
        controller:
            # Platform controller for the NSIF
            {% set cpm = 'CPM2' -%}
            name: &cpm2 {{ cpm }}
            refdes: {{ mooring }}-RIC21-00-CPMENG000
            parent: *nsif
            controlled_by: *cpm1
            physical: *nsif
            depth: *depth2
            uuid: {{ cpm2_uuid | default('UNKNOWN_UUID') }}
            serial: {{ cpm2_serial | default('UNKNOWN_SERIAL_NUMBER') }}
            raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ cpm | lower }}/superv/*.superv.log"
            parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/superv/{{ cpm | lower }}/*.superv.json"
            dataset: {{ mooring }}-NSIF-001-SUPERV
        beacons: None
        loggers:
            # Data loggers controlled by the NSIF platform controller
            - logger1:
                {% set dcl26 = 'DCL26' -%}
                name: &dcl26 {{ dcl26 }}
                refdes: {{ mooring }}-RID26-00-DCLENG000
                parent: *cpm2
