                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/adcpt/*.adcpt.json"
                dataset: {{ mooring }}-NSIF-001-ADCPT
            - velpt:
                name: VELPT
                refdes: {{ mooring }}-RID26-04-VELPTA000
                parent: *dcl26
                controlled_by: *dcl26
                physical: *nsif
                depth: *depth2
                port: 4
                uuid: {{ nsif_velpt_uuid | default('UNKNOWN_UUID') }}
                serial: {{ nsif_velpt_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl26 | lower }}/velpt1/*.velpt1.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/velpt/*.velpt1.json"
                dataset: {{ mooring }}-NSIF-001-VELPT
            - phsen:
                name: PHSEN
                refdes: {{ mooring }}-RID26-06-PHSEND000
                parent: *dcl26
                controlled_by: *dcl26
                physical: *nsif
                depth: *depth2
                port: 6
                uuid: {{ nsif_phsen_uuid | default('UNKNOWN_UUID') }}
                serial: {{ nsif_phsen_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl26 | lower }}/phsen1/*.phsen1.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/phsen/*.phsen1.json"
                dataset: {{ mooring }}-NSIF-001-PHSEN
            - nutnr:
                name: NUTNR
                refdes: {{ mooring }}-RID26-07-NUTNRB000
                parent: *dcl26
                controlled_by: *dcl26
