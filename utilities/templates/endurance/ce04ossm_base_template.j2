                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl11 | lower }}/hyd1/*.hyd1.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/hydgn1/*.hyd1.json"
                dataset: {{ mooring }}-BUOY-001-HYDGN
            - velpt:
                name: VELPT
                refdes: {{ mooring }}-SBD11-04-VELPTA000
                parent: *dcl11
                controlled_by: *dcl11
                physical: *nsif
                depth: *depth1
                port: 4
                uuid: {{ buoy_velpt_uuid | default('UNKNOWN_UUID') }}
                serial: {{ buoy_velpt_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl11 | lower }}/velpt1/*.velpt1.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/velpt/*.velpt1.json"
                dataset: {{ mooring }}-BUOY-001-VELPT
            - metbk:
                name: &metbk METBK
                refdes: {{ mooring }}-SBD11-06-METBKA000
                parent: *dcl11
                controlled_by: *dcl11
                physical: *buoy
                depth: *depth1
                port: 6
                uuid: {{ metbk_uuid | default('UNKNOWN_UUID') }}
                serial: {{ metbk_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl11 | lower }}/metbk/*.metbk.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/buoy/metbk/*.metbk.json"
                dataset: {{ mooring }}-BUOY-001-METBK
            - metct:
                name: METCT
                refdes: {{ mooring }}-SBD11-06-METBKA001
                parent: *metbk
                controlled_by: *metbk
                physical: *buoy
                depth: 1.5
                ui: False
                uuid: {{ metct_uuid | default('UNKNOWN_UUID') }}
                serial: {{ metct_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: None
                parsed_data: None
