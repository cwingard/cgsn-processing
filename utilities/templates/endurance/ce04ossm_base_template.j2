                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/spkir/*.spkir.json"
                dataset: {{ mooring }}-NSIF-001-SPKIR
            # Instruments controlled and logged by the second NSIF data logger
            - optaa:
                name: OPTAA
                refdes: {{ mooring }}-RID27-01-OPTAAD000
                parent: *dcl27
                controlled_by: *dcl27
                physical: *nsif
                depth: *depth2
                port: 1
                uuid: {{ nsif_optaa_uuid | default('UNKNOWN_UUID') }}
                serial: {{ nsif_optaa_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl27 | lower }}/optaa1_decimated/*.optaa1.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/optaa/*.optaa1.json"
                dataset: {{ mooring }}-NSIF-001-OPTAA
            - flort:
                name: FLORT
                refdes: {{ mooring }}-RID27-02-FLORTD000
                parent: *dcl27
                controlled_by: *dcl27
                physical: *nsif
                depth: *depth2
                port: 2
                uuid: {{ flort_uuid | default('UNKNOWN_UUID') }}
                serial: {{ flort_serial | default('UNKNOWN_SERIAL_NUMBER') }}
                raw_data: "{{ mooring | lower }}/{{ deployment_name }}/cg_data/{{ dcl27 | lower }}/flort/*.flort.log"
                parsed_data: "{{ mooring | lower }}/{{ deployment_name }}/nsif/flort/*.flort.json"
                dataset: {{ mooring }}-NSIF-001-FLORT
            - ctdbp:
                name: CTDBP
                refdes: {{ mooring }}-RID27-03-CTDBPC000
                parent: *dcl27
                controlled_by: *dcl27
                physical: *nsif
